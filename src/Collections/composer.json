{
    "name": "kasi/collections",
    "description": "The Kasi Collections component.",
    "license": "MIT",
    "homepage": "https://kasiapp.com",
    "support": {
        "issues": "https://github.com/kasiapps/framework/issues",
        "source": "https://github.com/kasiapps/framework"
    },
    "require": {
        "php": "^8.3",
        "kasi/conditionable": "^1.0",
        "kasi/contracts": "^1.0",
        "kasi/macroable": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Kasi\\Support\\": ""
        },
        "files": [
            "functions.php",
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^7.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
