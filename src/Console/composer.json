{
    "name": "kasi/console",
    "description": "The Kasi Console component.",
    "license": "MIT",
    "homepage": "https://kasiapp.com",
    "support": {
        "issues": "https://github.com/kasiapps/framework/issues",
        "source": "https://github.com/kasiapps/framework"
    },
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*",
        "kasi/bus": "^1.0",
        "kasi/collections": "^1.0",
        "kasi/container": "^1.0",
        "kasi/contracts": "^1.0",
        "kasi/filesystem": "^1.0",
        "kasi/macroable": "^1.0",
				"kasi/prompts": "^1.0",
				"kasi/queue": "^1.0",
        "kasi/support": "^1.0",
        "kasi/view": "^1.0",
        "nunomaduro/termwind": "^2.0",
        "symfony/console": "^7.0.3",
        "symfony/polyfill-php83": "^1.31",
        "symfony/process": "^7.0.3"
    },
    "autoload": {
        "psr-4": {
            "Kasi\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "ext-pcntl": "Required to use signal trapping.",
        "dragonmantank/cron-expression": "Required to use scheduler (^3.3.2).",
        "guzzlehttp/guzzle": "Required to use the ping methods on schedules (^7.8)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
