{
  "name": "kasi/tinker",
  "description": "The Kasi Tinker component.",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "kasi/console": "^1.0",
    "kasi/contracts": "^1.0",
    "kasi/database": "^1.0",
    "kasi/support": "^1.0",
    "psy/psysh": "^0.12.0",
    "symfony/var-dumper": "^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Kasi\\Tinker\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
