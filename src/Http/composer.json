{
    "name": "kasi/http",
    "description": "The Kasi Http component.",
    "license": "MIT",
    "homepage": "https://kasiapp.com",
    "support": {
        "issues": "https://github.com/kasiapps/framework/issues",
        "source": "https://github.com/kasiapps/framework"
    },
    "require": {
        "php": "^8.3",
        "ext-filter": "*",
        "fruitcake/php-cors": "^1.3",
        "guzzlehttp/guzzle": "^7.8.2",
        "guzzlehttp/uri-template": "^1.0",
        "kasi/collections": "^1.0",
        "kasi/macroable": "^1.0",
        "kasi/session": "^1.0",
        "kasi/support": "^1.0",
        "symfony/http-foundation": "^7.0.3",
        "symfony/http-kernel": "^7.0.3",
        "symfony/polyfill-php83": "^1.31",
        "symfony/mime": "^7.0.3"
    },
    "autoload": {
        "psr-4": {
            "Kasi\\Http\\": ""
        }
    },
    "suggest": {
        "ext-gd": "Required to use Kasi\\Http\\Testing\\FileFactory::image()."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
