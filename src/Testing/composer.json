{
    "name": "kasi/testing",
    "description": "The Kasi Testing component.",
    "license": "MIT",
    "homepage": "https://kasiapp.com",
    "support": {
        "issues": "https://github.com/kasiapps/framework/issues",
        "source": "https://github.com/kasiapps/framework"
    },
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*",
        "kasi/collections": "^1.0",
        "kasi/console": "^1.0",
        "kasi/contracts": "^1.0",
        "kasi/database": "^1.0",
        "kasi/http": "^1.0",
        "kasi/macroable": "^1.0",
        "kasi/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kasi\\Testing\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "brianium/paratest": "Required to run tests in parallel (^7.0|^8.0).",
        "mockery/mockery": "Required to use mocking (^1.6).",
        "phpunit/phpunit": "Required to use assertions and run tests (^10.5.35|^11.3.6|^12.0.1)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
